{
  "parser":"babel-eslint",
  "parserOptions": {
    "ecmaFeatures":{
      "jsx":true,
      "modules":true
    }
  },
  "globals":{
    "newrelic":true
  },
  "env":{
    "browser":true,
    "es6":true,
    "node":true
  },
  "extends":"eslint:recommended",
  "plugins":[
    "react"
  ],
  "rules": {
    "strict": 0,
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  }
}
